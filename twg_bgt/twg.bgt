int get_sequence(const string s, const bool only_alpha=false) {
int o=1;
if (s.length()<=1)
return -1;
for(uint i=0; i<s.length(); i++) {
for(uint ii=i+1; ii<s.length(); ii++) {
if (s[i]==s[ii]) {
if (only_alpha and string_is_alphabetic(s[i]) or !only_alpha)
o++;
break;
}
else
return o;
}
}
return o;
}
