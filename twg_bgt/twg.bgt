void fallloop()
{
if(jumptimer.elapsed > jumptime and jumping==true)
{
jumptimer.restart();
if(jumpup==1)
{
if(me.z <=jumplandz+5)
{
me.z++;
send_reliable(peer_id, "move "+me.x+" "+me.y+" "+me.z,8);
if(get_tile_at(me.x, me.y, me.z)!="" and get_tile_at(me.x, me.y, me.z)!="air")
{
jumpup=0;
}
}
else
{
jumpup=0;
}
}
else if(jumpup==0)
{
if(me.z > jumplandz)
{
me.z--;
send_reliable(peer_id, "move "+me.x+" "+me.y+" "+me.z,8);
if(get_tile_at(me.x, me.y, me.z)!="" and get_tile_at(me.x, me.y, me.z)!="air")
{
p.play_stationary(get_tile_at2(me.x, me.y, me.z)+"land.ogg",false,0,0,tilevolume,tilepitch);
send_reliable(peer_id,"draw2 "+get_tile_at2(me.x, me.y, me.z)+"land.ogg tilevolume tilepitch", 7);
jumping=false;
}
}
else
{
if(get_tile_at(me.x, me.y, me.z)=="" or get_tile_at(me.x, me.y, me.z)=="air" and frozen==false)
{
falling=true;
falldistance=0;
falltimer.restart();
jumping=false;
return;
}
p.play_stationary(get_tile_at2(me.x, me.y, me.z)+"land.ogg",false,0,0,tilevolume,tilepitch);
send_reliable(peer_id,"draw2 "+get_tile_at2(me.x, me.y, me.z)+"land.ogg tilevolume tilepitch", 7);
jumping=false;
movecheck();
}
}
}
}
void fallcheck()
{
if(dmoving==false and amoving==false)
{
if(get_tile_at(me.x, me.y, me.z)=="" and falling==false and playing==true and jumping==false or get_tile_at(me.x, me.y, me.z)=="air" and falling==false and jumping==false and playing==true)
{
falling=true;
falldistance=0;
falltimer.restart();
p.play_stationary("fall"+random(1,3)+".ogg",false,0,0,0,100);
send_reliable(peer_id,"draw fall"+random(1,3)+".ogg", 7);
send_reliable(peer_id, "fallstart", 8);
}
}
}
void fallingloop()
{
if(dmoving==false and amoving==false)
{
if(falling==true and falltimer.elapsed > falltime)
{
if(get_tile_at(me.x, me.y, me.z)!="" and get_tile_at(me.x, me.y, me.z)!="air")
{
falling=false;
if(falldistance < 5)
{
send_reliable(peer_id,"fallstop",8);
p.play_stationary(get_tile_at2(me.x, me.y, me.z)+"land.ogg",false,0,0,tilevolume,tilepitch);
send_reliable(peer_id,"draw2 "+get_tile_at2(me.x, me.y, me.z)+"land.ogg tilevolume tilepitch", 7);
send_reliable(peer_id, "move "+me.x+" "+me.y+" "+me.z,8);
falling=false;
movecheck();
}
else
{
send_reliable(peer_id,"fallstop",8);
p.play_stationary(get_tile_at2(me.x, me.y, me.z)+"fall.ogg",false,0,0,tilevolume,tilepitch);
send_reliable(peer_id,"draw2 "+get_tile_at2(me.x, me.y, me.z)+"fall.ogg  tilevolume tilepitch", 7);
send_reliable(peer_id, "move "+me.x+" "+me.y+" "+me.z,8);
send_reliable(peer_id, "hardland "+falldistance, 8);
falling=false;
movecheck();
}
return;
}
falltimer.restart();
falldistance++;
me.z--;
send_reliable(peer_id, "move "+me.x+" "+me.y+" "+me.z,8);
movecheck();
}
}
